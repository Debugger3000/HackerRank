


public static int birthday(List<int> s, int d, int m)
    {
        int numb = 0;
        
        for(int i = 0; i <= s.Count - m; i++){
            int temp = 0;
            for(int z = i; z < m+i; z++){
                
                temp = temp + s[z];
            }
            
            if(temp == d){
                numb++;
            }
        
        }
        
        return numb;
    }
